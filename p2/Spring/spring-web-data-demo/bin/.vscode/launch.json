{
    "configurations": [
        {
            "type": "java",
            "name": "Launch SpringWebDataDemoApplication",
            "request": "launch",
            "mainClass": "com.revature.springwebdatademo.SpringWebDataDemoApplication",
            "projectName": "spring-web-data-demo"
        },
        {
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "name": "Launch Extension",
            "outFiles": [
                "${helloworld}/out/**/*.js"
            ],
            "preLaunchTask": "npm",
            "request": "launch",
            "type": "extensionHost"
        },
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${helloworld}/bin/Debug/<target-framework>/<project-helloworld.dll>",
            "args": [],
            "cwd": "${helloworld}",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${hellowo}/Views"
            }
        }
    ]
}